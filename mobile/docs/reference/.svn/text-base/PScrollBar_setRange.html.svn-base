<table border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td class="reffieldheader">Name</td>
    <td class="reffield">
      <h3>setRange()</h3>
    </td>
  </tr>
  <tr>
    <td class="reffieldheader">Description</td>
    <td class="reffield">
      Sets the range of values represented by the scrollbar, and 
the size of a "page" used to size the scroller.    </td>
  </tr>
  <tr>
    <td class="reffieldheader">Syntax</td>
    <td class="reffield">
      <pre>setRange(<c>min</c>, <c>max</c>, <c>pageSize</c>);</pre>
    </td>
  </tr>
  <tr>
    <td class="reffieldheader">Parameters</td>
    <td class="reffield">
      <table border="0" cellspacing="0" cellpadding="0">
                        <tr>
                   <td valign="top" width="70">min</td>
                   <td width="20">&nbsp;</td>
                   <td valign="top">the minimum value<br><br></td>
                 </tr>
                        <tr>
                   <td valign="top" width="70">max</td>
                   <td width="20">&nbsp;</td>
                   <td valign="top">the maximum value<br><br></td>
                 </tr>
                        <tr>
                   <td valign="top" width="70">pageSize</td>
                   <td width="20">&nbsp;</td>
                   <td valign="top">the size of a page<br><br></td>
                 </tr>
             </table>
    </td>
  </tr>
</table>

