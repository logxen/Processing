<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root>
<name>volume()</name>

<description>
Sets the volume of the sound. The volume ranges from 0 (mute) to 100 (loudest).
</description>

<syntax>
sound.volume(<c>level</c>)
</syntax>

<parameter>
<label>sound</label>
<description>Sound: any variable of type Sound</description>
</parameter>

<parameter>
<label>level</label>
<description>int: the volume level of the sound</description>
</parameter>

<returns>None</returns>

<related>
</related>

<availability>1.0</availability>

<type></type>

<partof>Sound</partof>


</root>
