<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root>
<name>call()</name>

<description>
Calls the specified phone number. Returns true if the sketch must terminate before the call will be initiated.
</description>

<syntax>
p.call(<c>number</c>)
</syntax>

<parameter>
<label>p</label>
<description>Phone: any variable of type Phone</description>
</parameter>

<parameter>
<label>number</label>
<description>String: phone number to dial</description>
</parameter>

<returns>boolean</returns>

<related>
</related>

<availability>1.0</availability>

<type></type>

<partof>Net</partof>


</root>
