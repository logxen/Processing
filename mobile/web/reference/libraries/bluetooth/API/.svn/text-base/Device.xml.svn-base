<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root>
<name>Device</name>

<description>
Objects of this class represent nearby devices discovered on the Bluetooth network.
</description>

<field>
<fname>name</fname>
<fdescription>The name of the device. The name may be (Unknown) until the discovery process is complete.</fdescription>
</field>

<field>
<fname>address</fname>
<fdescription>A string uniquely identifying the device on the network.</fdescription>
</field>

<method>
<mname>cancel()</mname>
<mdescription>Cancels a search for services.</mdescription>
</method>

<method>
<mname>discover()</mname>
<mdescription>Searches for services on the device.</mdescription>
</method>

<returns></returns>

<related>
</related>

<availability>1.0</availability>

<type>Object</type>

<partof>Bluetooth</partof>

</root>
