<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root>

<name>stop()</name>

<category>Capture</category>

<subcategory></subcategory>

<usage>Web &amp; Application</usage>

<example>
<image></image>
<code><![CDATA[
import processing.video.*;
Capture myCapture;

void setup() 
{
  size(200, 200);
  String s = "Logitech QuickCam Messenger-WDM";
  myCapture = new Capture(this, s, width, height, 30);
}

void draw() {
  if (myCapture.available()) {
    myCapture.read();
  }
  image(myCapture, 0, 0);
}

void mousePressed() {
  myCapture.stop();
}

]]></code>
</example>

<description><![CDATA[
Stops capturing frames from an attached device.
]]></description>

<syntax>
<c>cam</c>.stop()
</syntax>

<parameter>
<label>cam</label>
<description><![CDATA[any variable of type Capture]]></description>
</parameter>

<returns></returns>

<related></related>

<availability>1.0</availability>

